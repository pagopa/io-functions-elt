{  
  "version": "2.0",
  "functionTimeout": "02:30:00",
  "logging": {
    "logLevel": {
      "default": "Trace"
    }
  },
  "extensions": {
    "cosmosDB": {
      "connectionMode": "Gateway",
      "protocol": "Https"
    },
    "eventHubs": {
      "initialOffsetOptions": {
          "type": "fromEnd",
          "enqueuedTimeUtc": ""
      }
    },
    "queues": {
      "maxDequeueCount": 31,
      "visibilityTimeout": "00:00:05"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}
